        <Button fx:id="button_send" layoutX="398.0" layoutY="336.0" prefHeight="27.0" prefWidth="50.0" text="Send" />
        <TextField fx:id="tf_message" layoutX="30" layoutY="338" prefHeight="26" prefWidth="361" />
        <ScrollPane fx:id="sp_main" fitToWidth="true" layoutX="30" layoutY="70" prefHeight="256" prefWidth="418">
            <content>
                <VBox fx:id="vbox_messages" prefHeight="248" prefWidth="412" />
            </content>
        </ScrollPane>
        <Label layoutX="177.0" layoutY="26.0" text="GroupChat">
            <font>
                <Font name="Arial Rounded MT Bold" size="23" />
            </font>
        </Label>
      <Button fx:id="btn_logout" layoutX="368.0" layoutY="4.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="110.0" style="-fx-background-color: transparent; -fx-cursor: hand;" text="Disconnect" textFill="#2900ff" underline="true">
